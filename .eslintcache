\r\n    const {training} = useSelector(state => state.training)\r\n\r\n    useEffect(() => {\r\n        \r\n        dispatch(startTrainingDay())\r\n        \r\n\r\n    }, [dispatch])\r\n\r\n    return (\r\n        <div>\r\n            \r\n        </div>\r\n    )\r\n}\r\n","C:\\00.Principal\\00.Importante\\04.Proyectos\\01.Gym\\gimnasio\\gym_react\\src\\components\\ui\\Navbar.jsx",[],"C:\\00.Principal\\00.Importante\\04.Proyectos\\01.Gym\\gimnasio\\gym_react\\src\\types\\types.js",[],"C:\\00.Principal\\00.Importante\\04.Proyectos\\01.Gym\\gimnasio\\gym_react\\src\\components\\exercises\\ExerciseScreen.jsx",["141"],"import React, { useEffect, useState } from 'react'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport { useParams } from 'react-router-dom';\r\nimport { startExercises } from '../../actions/exercises';\r\nimport { Exercises } from './Exercises';\r\n\r\nexport const ExerciseScreen = () => {\r\n\r\n    const { muscle } = useParams();\r\n    \r\n    const dispatch = useDispatch()\r\n    const {exercises} = useSelector(state => state.exercise)\r\n    useEffect(() => {\r\n        \r\n        dispatch(startExercises(muscle))\r\n\r\n       \r\n    }, [dispatch,muscle])\r\n\r\n    \r\n    return (\r\n        <div style={{\r\n            flexDirection: 'row'\r\n        }}>\r\n            Pagina ejercicios\r\n            <input type=\"date\"/>\r\n            {\r\n                exercises.ejercicios && \r\n                exercises.ejercicios.map((ejercicio) => {\r\n                return (\r\n                    <Exercises key={ejercicio.id} {...ejercicio} />\r\n                )})\r\n                  \r\n                }\r\n        </div>\r\n    )\r\n}\r\n","C:\\00.Principal\\00.Importante\\04.Proyectos\\01.Gym\\gimnasio\\gym_react\\src\\actions\\exercises.js",["142","143"],"import { fetchConToken, fetchSinToken } from \"../helpers/fetch\";\r\nimport { types } from \"../types/types\";\r\nimport Swal from \"sweetalert2\";\r\nimport { useSelector } from \"react-redux\";\r\n\r\n\r\n\r\nexport const startExercises = (muscle) => {\r\nreturn async (dispatch) => {\r\n    try {\r\n        \r\n        const resp = await fetchConToken(`exercises/${muscle}`)\r\n        const exercises = await resp.json()\r\n        \r\n        dispatch(setExercises(exercises))\r\n    } catch (error) {\r\n        console.log(error)\r\n    }\r\n}\r\n\r\n}\r\n\r\nexport const startTrainingDay = () =>{\r\n    return async (dispatch) => {\r\n        try{\r\n            const { uid } = useSelector((state) => state.auth);\r\n            const resp = await fetchConToken(`training/${uid}/${Date.now().toString()}`)\r\n            const trainingday = await resp.json();\r\n\r\n            dispatch(setTrainingDay(trainingday))\r\n        }catch(error){\r\n            console.log(error)\r\n        }\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\nexport const startActiveTraining = (id,  nombre,rutina) => {\r\n    return(dispatch) => {\r\n    const exercise = {\r\n        id,nombre, rutina\r\n    }\r\n\r\n    dispatch(exerciseCreateTraining(exercise))\r\n\r\n    // fetchConToken(`training/${id}/${Date.now().toString()}`, exercise, \"POST\")\r\n\r\n\r\n\r\n}\r\n}\r\nexport const startAddColumnActiveTraining = (id,rutina) => {\r\n    \r\n    return(dispatch) => {\r\n        \r\n        const exercise = {\r\n            id, rutina\r\n        }\r\n    \r\n        dispatch(exerciseUpdateActiveTraining(exercise))\r\n    }\r\n}\r\n\r\nconst setExercises = (exercises) =>( {\r\n    type: types.exerciseShowExercises,\r\n    payload: exercises\r\n})\r\n\r\nconst exerciseCreateTraining = (exercise) => ({\r\n\r\n    type: types.exerciseSetActiveTraining,\r\n    payload: exercise\r\n\r\n})\r\n\r\nconst setTrainingDay = (exercises) => (\r\n{\r\n    type: types.trainingDay,\r\n    payload: exercises\r\n}\r\n\r\n)\r\n\r\nconst exerciseUpdateActiveTraining = (exercise) => ({\r\n\r\n    type: types.exerciseUpdateActiveTraining,\r\n    payload: exercise\r\n\r\n})","C:\\00.Principal\